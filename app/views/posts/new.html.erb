<h1>New Post</h1>

<div class="new-post-form">
  <%= form_with(model: [@user, @post], url: user_posts_path(@user)) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
  
    <%= form.label :text %>
    <%= form.text_area :text %>
  
    <%= form.submit "Create Post" %>
  <% end %>
</div>